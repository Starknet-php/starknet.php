{
    "name": "starknet-php/starknet.php",
    "description": "PHP sdk that allows you to interact with StarkNet L2 network",
    "keywords": ["php", "StarkNet", "ethereum", "L2", "sdk"],
    "license": "MIT",
    "authors": [
        {
            "name": "Zac Whittaker",
            "email": "asciik@protonmail.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "ext-bcmath": "*",
        "guzzlehttp/guzzle": "^7.4",
        "kornrunner/keccak": "^1.1",
        "mdanter/ecc": "^1.0",
        "phpseclib/phpseclib": "^3.0.12",
        "simplito/elliptic-php": "^1.0"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.3.2",
        "mockery/mockery": "^1.4.4",
        "pestphp/pest": "^1.21.1",
        "phpstan/phpstan": "^1.2.0",
        "symfony/var-dumper": "^5.4.0"
    },
    "autoload": {
        "psr-4": {
            "starknet\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "preferred-install": "dist"
    },
    "scripts": {
        "lint": "",
        "test:lint": "",
        "test:types": "",
        "test:unit": "",
        "test": [
            "@test:lint",
            "@test:types",
            "@test:unit"
        ]
    }
}
